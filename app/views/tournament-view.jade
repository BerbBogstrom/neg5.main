extends layout

block titleContent
    title #{tournament.tournament_name} | Neg 5

block extrastylesheets
    link(rel="stylesheet" href="/css/home.css")
    link(rel="stylesheet" href="/css/tournamentstyle.css")
    link(rel="stylesheet" href="/css/chat.css")

block content

    +navbar

